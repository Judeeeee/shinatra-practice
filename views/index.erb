<div class="memo-list">
  <h2>メモリスト</h2>
  <% @memos.each do |memo| %>
    <% @id = memo["id"] %>
    <% @title = memo["title"] %>
    <p><a href=/memos/<%= @id %>> <%= html_escape(@title) %></a><br></p>
  <% end %>
</div>
<div class="new-botton">
  <a href="/memos/new">
    <button type="submit">新規作成</button>
  </a>
</div>
